{
  "name": "@feflow/packager",
  "version": "0.2.3",
  "description": "Feflow packager",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "bin",
    "src",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tencent/feflow.git"
  },
  "scripts": {
    "start": "npm run build:live",
    "clean": "rimraf lib",
    "compile": "tsc -p tsconfig.build.json",
    "prebuild": "npm run clean",
    "build": "npm run compile",
    "test": "nyc mocha -r ts-node/register __tests__/**/*.test.ts",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/cross-spawn": "^6.0.0",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "@types/semver": "^6.0.1",
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^2.0.27",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cross-spawn": "^6.0.5",
    "execa": "^4.0.0",
    "get-stream": "^5.1.0",
    "minimist": "^1.2.0"
  },
  "engines": {
    "node": ">=8.0"
  },
  "license": "MIT",
  "bin": {
    "pack": "./bin/pack"
  },
  "keywords": [
    "feflow",
    "front-end",
    "flow"
  ],
  "nyc": {
    "include": [
      "test/**/*.spec.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "bugs": {
    "url": "https://github.com/Tencent/feflow/issues"
  },
  "gitHead": "834a44fcd9e9168483282d73d955c089dedcec5b"
}
