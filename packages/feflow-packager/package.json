{
  "name": "@feflow/packager",
  "version": "0.1.0",
  "description": "Feflow packager",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "bin",
    "src",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tencent/feflow.git"
  },
  "scripts": {
    "start": "npm run build:live",
    "build": "rm -rf lib && tsc",
    "test": "nyc mocha -r ts-node/register __tests__/**/*.test.ts",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cross-spawn": "^6.0.5",
    "execa": "^4.0.0",
    "get-stream": "^5.1.0",
    "minimist": "^1.2.0"
  },
  "engines": {
    "node": ">=8.0"
  },
  "license": "MIT",
  "bin": {
    "pack": "./bin/pack"
  },
  "keywords": [
    "feflow",
    "front-end",
    "flow"
  ],
  "nyc": {
    "include": [
      "test/**/*.spec.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "bugs": {
    "url": "https://github.com/Tencent/feflow/issues"
  },
  "gitHead": "834a44fcd9e9168483282d73d955c089dedcec5b"
}
