    "@types/request-promise": "^4.1.48",
    "chai": "^4.2.0",
    "eslint": "^6.5.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.9.10"
  },
  "files": [
    "lib",
    "types"
  ],
