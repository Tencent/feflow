{
  "name": "@feflow/feflow-plugin-devtool",
  "version": "0.4.0-alpha.3",
  "description": "Feflow internal plugins",
  "main": "./lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tencent/feflow.git"
  },
  "scripts": {
    "start": "npm run build:live",
    "clean": "rimraf lib",
    "compile": "tsc -p tsconfig.build.json",
    "checkType": "tsc --noEmit",
    "prebuild": "npm run checkType && npm run clean",
    "build": "npm run compile",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "@feflow/cli": "^0.26.0-alpha.3",
    "@types/fs-extra": "^8.1.2",
    "@types/inquirer": "^6.5.0",
    "@types/node": "^18.7.2",
    "chalk": "^2.4.2",
    "fs-extra": "^8.1.0",
    "inquirer": "^6.5.0"
  },
  "engines": {
    "node": ">=8.0"
  },
  "license": "MIT",
  "keywords": [
    "feflow",
    "front-end",
    "flow"
  ],
  "nyc": {
    "include": [
      "test/**/*.spec.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "bugs": {
    "url": "https://github.com/Tencent/feflow/issues"
  },
  "gitHead": "834a44fcd9e9168483282d73d955c089dedcec5b"
}
