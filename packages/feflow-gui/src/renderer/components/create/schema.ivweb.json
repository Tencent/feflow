{
  "gererator": "generatorIvwebStartkit",
  "description": "Now直播基础脚手架",
  "properties": [
    {
      "ascription": "basicInfo",
      "type": "input",
      "title": "rtx",
      "field": "rtxName"
    },
    {
      "ascription": "basicInfo",
      "type": "input",
      "title": "项目名字",
      "field": "name"
    },
    {
      "ascription": "basicInfo",
      "type": "input",
      "title": "项目的中文别称，用于项目之间的识别，请尽量避免与其他项目重名，限制5-15个汉字",
      "field": "nickName"
    },
    {
      "ascription": "basicInfo",
      "type": "input",
      "title": "版本号",
      "field": "version"
    },
    {
      "ascription": "basicInfo",
      "type": "input",
      "title": "项目描述",
      "field": "description",
      "description": "推荐使用5-10个汉字"
    },
    {
      "ascription": "git",
      "field": "isRequiredRepo",
      "type": "switch",
      "title": "是否创建Git仓库",
      "default": true
    },
    {
      "ascription": "git",
      "field": "isCreateRemoteRepoSuccess",
      "type": "switch",
      "title": "是否成功创建了远程仓库",
      "default": false
    },
    {
      "ascription": "aegis",
      "field": "isRequiredAegis",
      "type": "switch",
      "title": "是否需要自动申请接入 Aegis Id",
      "default": true
    },
    {
      "ascription": "aegis",
      "field": "aegisId",
      "type": "input",
      "title": "如果已经存在了 aegisId 的话，就设置一下",
      "default": 0
    },
    {
      "ascription": "aegis",
      "field": "isApplyAegisSuccess",
      "type": "switch",
      "title": "是否成功申请了 aegisId",
      "default": false
    },
    {
      "ascription": "basicInfo",
      "field": "version",
      "type": "input",
      "title": "是否创建Git仓库",
      "default": "v0.1.0"
    }
  ],
  "isRequire": ["projectName"],
  "author": "bethon"
}
