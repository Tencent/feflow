{
  "name": "@feflow/cli",
  "version": "0.20.7",
  "description": "A front-end flow tool.",
  "main": "./lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "bin",
    "src",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tencent/feflow.git"
  },
  "scripts": {
    "start": "npm run build:live",
    "build": "rm -rf lib && tsc",
    "test": "nyc mocha -r ts-node/register __tests__/**/*.test.ts",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/cli/index.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepublish": "npm run build"
  },
  "oclif": {
    "bin": "fef",
    "macos": {
      "identifier": "com.feflow.cli"
    }
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/abbrev": "^1.1.0",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/cross-spawn": "^6.0.0",
    "@types/easy-table": "0.0.32",
    "@types/execa": "^2.0.0",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^6.0.3",
    "@types/js-yaml": "^3.12.1",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "@types/osenv": "^0.1.0",
    "@types/request-promise": "^4.1.45",
    "@types/semver": "^6.0.1",
    "@types/yeoman-environment": "^2.3.1",
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^2.0.27",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prettier": "2.0.5",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@feflow/feflow-plugin-devtool": "0.0.1",
    "@feflow/report": "^0.1.8",
    "@types/execa": "^2.0.0",
    "abbrev": "^1.1.1",
    "bunyan": "^1.8.12",
    "chalk": "^2.4.2",
    "command-line-usage": "^6.1.0",
    "commander": "^2.20.0",
    "cross-spawn": "^6.0.5",
    "easy-table": "^1.1.1",
    "execa": "^4.0.1",
    "figlet": "^1.2.3",
    "import-fresh": "^3.1.0",
    "inquire": "^0.4.8",
    "inquirer": "^6.5.0",
    "js-yaml": "^3.13.1",
    "lookpath": "^1.0.6",
    "minimist": "^1.2.0",
    "osenv": "^0.1.5",
    "package-json": "^6.5.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "semver": "^7.3.2",
    "strip-json-comments": "^3.0.1",
    "yeoman-environment": "^2.4.0"
  },
  "engines": {
    "node": ">=8.0"
  },
  "license": "MIT",
  "bin": {
    "fef": "./bin/feflow"
  },
  "keywords": [
    "feflow",
    "front-end",
    "flow"
  ],
  "nyc": {
    "include": [
      "test/**/*.spec.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "bugs": {
    "url": "https://github.com/Tencent/feflow/issues"
  }
}
